{
  "module-exports-template": {
    "key": "Ctrl+Alt+E",
    "prefix": "elf-module-exports-template",
    "body": [
      "module.exports = {",
      "\tentry: {",
      "\t\tvendor: \"./src/index.js$0\"",
      "\t},",
      "\tmode: \"development\",",
      "\tmodule: {",
      "\t\trules: []",
      "\t},",
      "\tplugins: [],",
      "\toutput: {",
      "\t\t$1filename: \"bundle.js\",",
      "\t\tpath: path.resolve(__dirname,\"dist\")",
      "\t},",
      "}"
    ],
    "description": "For Loop"
  },
  "dev-server": {
    "prefix": "elf-dev-server",
    "body": [
      "devServer: {",
      "\tcontentBase: \"./dist\",",
      "\thot: true",
      "},"
    ]
  },
  "source-map": {
    "prefix": "elf-source-map",
    "body": [
      "devtool: \"source-map\""
    ]
  },
  "inline-source-map": {
    "prefix": "elf-inline-source-map",
    "body": [
      "devtool: \"inline-source-map\","
    ]
  },
  "resolve-js-ts": {
    "prefix": "elf-resolve-js-ts",
    "body": [
      "resolve:{",
      "\textensions: ['.js','jsx','.ts', '.tsx' ]",
      "}"
    ]
  },
  "babel-loader": {
    "prefix": "elf-babel-loader",
    "body": [
      "{",
      "\ttest: /.js$/,",
      "\tuse: {",
      "\t\tloader: \"babel-loader\",",
      "\t\toptions: {",
      "\t\t\tpresets: [\"stage-0\", \"react\"],",
      "\t\t\tplugins: [$0]",
      "\t\t}",
      "\t}",
      "}"
    ]
  },
  "css-loader": {
    "prefix": "elf-css-loader",
    "body": [
      "{",
      "\ttest: /.css$/,",
      "\tuse: [\"style-loader\",\"css-loader\"]",
      "}"
    ]
  },
  "ts-loader": {
    "prefix": "elf-ts-loader",
    "body": [
      "{",
      "\ttest: /\\.tsx?$/,",
      "\tloader: 'ts-loader'",
      "}"
    ]
  },
  "optimization": {
    "prefix": "elf-optimization",
    "body": [
      "optimization: {",
      "\tsplitChunks: {",
      "\t\tcacheGroups: {",
      "\t\t\tcommons: {",
      "\t\t\t\tname: \"commons\",",
      "\t\t\t\tchunks: \"initial\"",
      "\t\t\t}",
      "\t\t}",
      "\t}",
      "},"
    ]
  },
  "requireHTMLPlugin": {
    "prefix": "elf-requireHTMLPlugin",
    "body": [
      "var HTMLWebpackPlugin = require(\"html-webpack-plugin\");"
    ]
  },
  "requireCleanPlugin": {
    "prefix": "elf-requireCleanPlugin",
    "body": [
      "var CleanWebpackPlugin = require(\"clean-webpack-plugin\");"
    ]
  },
  "cleanPlugin": {
    "prefix": "elf-clean-plugin",
    "body": [
      "new CleanWebpackPlugin([\"dist\"]),"
    ]
  },
  "HTMLPlugin": {
    "prefix": "elf-html-plugin",
    "body": [
      "new HTMLWebpackPlugin({ title: \"$0Title\", template: \"./src/index.html\" }),"
    ]
  }
}